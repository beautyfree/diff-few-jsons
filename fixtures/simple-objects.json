{
  "simple-objects": {
    "before": {
      "name": "John",
      "age": 30,
      "city": "New York"
    },
    "after": {
      "name": "John",
      "age": 31,
      "city": "Boston"
    }
  },
  "nested-objects": {
    "before": {
      "user": {
        "id": 1,
        "profile": {
          "firstName": "John",
          "lastName": "Doe",
          "email": "john@example.com"
        }
      }
    },
    "after": {
      "user": {
        "id": 1,
        "profile": {
          "firstName": "John",
          "lastName": "Smith",
          "email": "john.smith@example.com"
        }
      }
    }
  },
  "array-changes": {
    "before": {
      "items": [
        "apple",
        "banana",
        "cherry"
      ]
    },
    "after": {
      "items": [
        "apple",
        "blueberry",
        "cherry",
        "date"
      ]
    }
  },
  "object-array": {
    "before": {
      "users": [
        {
          "id": 1,
          "name": "Alice"
        },
        {
          "id": 2,
          "name": "Bob"
        },
        {
          "id": 3,
          "name": "Charlie"
        }
      ]
    },
    "after": {
      "users": [
        {
          "id": 1,
          "name": "Alice"
        },
        {
          "id": 2,
          "name": "Robert"
        },
        {
          "id": 4,
          "name": "David"
        }
      ]
    }
  }
}
